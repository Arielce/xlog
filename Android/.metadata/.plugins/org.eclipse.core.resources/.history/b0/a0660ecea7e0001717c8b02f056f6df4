
package com.sinosun.mstplib2.log;

/**
 * 日志级别
 */
public enum Level {
    VERBOSE(6), DEBUG(5), INFO(4), WARN(3), ERROR(2), FATAL(1), NONE(0);

    private int mValue;

    /**
     * 构造方法
     * 
     * @param value
     */
    private Level(int value) {
        this.mValue = value;
    }

    /**
     * 日志级别对应的整数值
     * 
     * @return 级别对应的整数值
     */
    public int getValue() {
        return mValue;
    }

}
